<?xml version="1.0" encoding="utf-8"?>
<StripLayouts xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <StripLayout Name="Default" ArrowType="None">
    <StripLayoutSections>
      <StripLayoutSection FlowDirection="Horizontal" Border="Vertical">
        <StripLayoutSubsections>
          <StripLayoutSection FlowDirection="Vertical">
            <StripLayoutSubsections>
              <StripLayoutSection FlowDirection="Horizontal">
                <StripLayoutSubsections>
                  <StripLayoutSection FlowDirection="Horizontal" Border="Vertical">
                    <StripLayoutLines>
                      <StripLayoutLine>
                        <StripItems>
                          <StripSeparatorItem MinLength="3" />
                          <StripItem Type="Callsign" LeftClick="Strip_Extend" RightClick="Label_ACID_Menu" MaxLength="8" MinLength="8" FontSize="XtraLarge" />
                          <StripItem Type="OtherInfo" MaxLength="1" MinLength="1" FontSize="XtraLarge" />
                        </StripItems>
                      </StripLayoutLine>
                      <StripLayoutLine>
                        <StripItems>
                          <StripItem Type="AircraftType" LeftClick="Strip_Extend" RightClick="Label_ACID_Menu" MaxLength="4" MinLength="4" FontSize="XtraSmall" />
                          <StripSeparatorItem MinLength="2" />
                          <StripItem Type="WakeTurbCategory" FontSize="XtraSmall" />
                          <StripSeparatorItem MinLength="1" />
                          <StripItem Type="PerformanceCategory" FontSize="XtraSmall" />
                          <StripSeparatorItem MinLength="1" />
                          <StripItem Type="TAS" FontSize="XtraSmall" />
                        </StripItems>
                      </StripLayoutLine>
                      <StripLayoutLine>
                        <StripItems>
                          <StripItem Type="Departure" LeftClick="Label_Field18_Popup" MaxLength="4" MinLength="4" FontSize="Small" />
                          <StripSeparatorItem MinLength="3" />
                          <StripItem Type="PRL" LeftClick="Label_PRL_Popup" MaxLength="3" MinLength="3" FontSize="XtraSmall" />
                          <StripSeparatorItem MinLength="1" />
                          <StripItem Type="Destination" LeftClick="Label_Field18_Popup" MaxLength="4" MinLength="4" FontSize="Small" />
                        </StripItems>
                      </StripLayoutLine>
                    </StripLayoutLines>
                  </StripLayoutSection>
                  <StripLayoutSection FlowDirection="Horizontal" Border="Vertical">
                    <StripLayoutLines>
                      <StripLayoutLine>
                        <StripItems>
                          <StripItem Type="GlobalOpsInfo" LeftClick="Strip_GlobalOps" MinLength="3" MaxLength="3" FontSize="Large" />
                        </StripItems>
                      </StripLayoutLine>	
                      <StripLayoutLine>
                        <StripItems>
                          <StripItem Type="CFL" LeftClick="Label_CFL_Popup" MinLength="3" MaxLength="3" FontSize="Large" />
                        </StripItems>
                      </StripLayoutLine>
                      <StripLayoutLine>
                        <StripItems>
                          <StripItem Type="AssignedSSR" LeftClick="Strip_ASSR" MinLength="4" MaxLength="4" FontSize="Small"/>
                        </StripItems>
                      </StripLayoutLine>
                    </StripLayoutLines>
                  </StripLayoutSection>
                  <StripLayoutSection FlowDirection="Horizontal" Border="Vertical"> <Colours BackColour="GhostJurisdiction"/>
                    <StripLayoutSubsections>
                      <StripLayoutSection FlowDirection="Horizontal">
                        <StripLayoutLines>
                          <StripLayoutLine LineSpan="2">
                            <StripItems>
                              <StripItem Type="Point" LeftClick="Strip_Point" MiddleClick="Strip_MPR_Arm" RightClick="Strip_Point_Expand" PointIndex="0" MaxLength="6" MinLength="6" > <Colours Colour="Jurisdiction"/> </StripItem>
                            </StripItems>
                          </StripLayoutLine>
                          <StripLayoutLine>
                            <StripItems>
                              <StripItem Type="ETOP" LeftClick="Strip_ETO" PointIndex="0" MinLength="4"> <Colours Colour="Jurisdiction"/> </StripItem>
                            </StripItems>
                          </StripLayoutLine>
                        </StripLayoutLines>
                      </StripLayoutSection>
                      <StripLayoutSection FlowDirection="Horizontal" Border="Vertical"> <Colours BorderColour="StripText" Colour="StripBackground" BackColour="HighlightedText"/>
                        <StripLayoutLines>
                          <StripLayoutLine LineSpan="2">
                            <StripItems>
                              <StripItem Type="Point" LeftClick="Strip_Point" MiddleClick="Strip_MPR_Arm" RightClick="Strip_Point_Expand" PointIndex="1" MaxLength="6" MinLength="6" />
                            </StripItems>
                          </StripLayoutLine>
                          <StripLayoutLine>
                            <StripItems>
                              <StripItem Type="ETOP" LeftClick="Strip_ETO" PointIndex="1" MinLength="6" />
                            </StripItems>
                          </StripLayoutLine>
                        </StripLayoutLines>
                      </StripLayoutSection>
                      <StripLayoutSection FlowDirection="Horizontal" Border="Vertical">
                        <StripLayoutLines>
                          <StripLayoutLine LineSpan="2">
                            <StripItems>
                              <StripItem Type="Point" LeftClick="Strip_Point" MiddleClick="Strip_MPR_Arm" RightClick="Strip_Point_Expand" PointIndex="2" MaxLength="6" MinLength="6" > <Colours P4Colour="Unstable"/> </StripItem>
                            </StripItems>
                          </StripLayoutLine>
                          <StripLayoutLine>
                            <StripItems>
                              <StripItem Type="ETOP" LeftClick="Strip_ETO" PointIndex="2" MinLength="6" />
                            </StripItems>
                          </StripLayoutLine>
                        </StripLayoutLines>
                      </StripLayoutSection>
                      <StripLayoutSection FlowDirection="Horizontal" Border="Vertical" >
                        <StripLayoutLines>
                          <StripLayoutLine LineSpan="2">
                            <StripItems>
                              <StripItem Type="Point" LeftClick="Strip_Point" MiddleClick="Strip_MPR_Arm" RightClick="Strip_Point_Expand" PointIndex="3" MaxLength="6" MinLength="6" />
                            </StripItems>
                          </StripLayoutLine>
                          <StripLayoutLine>
                            <StripItems>
                              <StripItem Type="ETOP" LeftClick="Strip_ETO" PointIndex="3" MinLength="6" />
                            </StripItems>
                          </StripLayoutLine>
                        </StripLayoutLines>
                      </StripLayoutSection>		
                      <StripLayoutSection FlowDirection="Horizontal" Border="Vertical"> <Colours BackColour="StripBackground"/>
                        <StripLayoutLines>
                          <StripLayoutLine>
                            <StripItems>
								<StripItem Type="ControllingSector" LeftClick="Strip_Controller" RightClick="Strip_State" MaxLength="2" MinLength="2" />
                            </StripItems>				
                          </StripLayoutLine>
                          <StripLayoutLine LineSpan="2">
                            <StripItems>
                              <StripSeparatorItem MinLength="2" >	<Colours BackColour="StripBackground"/> </StripSeparatorItem>
                            </StripItems>
                          </StripLayoutLine>						  
                        </StripLayoutLines>					  
                      </StripLayoutSection>					  						 
					</StripLayoutSubsections>
				  </StripLayoutSection>	
                </StripLayoutSubsections>
			  </StripLayoutSection>
              <StripLayoutSection FlowDirection="Vertical"> 
                <StripLayoutSubsections>
                  <StripLayoutSection FlowDirection="Horizontal" Border="Left Top Bottom" DisplayState="Extended">
                    <StripLayoutLines>
                      <StripLayoutLine>
                        <StripItems>
                          <StripItem Type="OtherInfo" LeftClick="Label_Field18_Popup" MaxLength="52" MinLength="52" />
                        </StripItems>
                      </StripLayoutLine>
                    </StripLayoutLines>
                  </StripLayoutSection>
                </StripLayoutSubsections>
              </StripLayoutSection>
            </StripLayoutSubsections>
          </StripLayoutSection>
        </StripLayoutSubsections>
	  </StripLayoutSection>
    </StripLayoutSections>
  </StripLayout>
</StripLayouts>